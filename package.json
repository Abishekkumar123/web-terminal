{
    "name": "web-terminal",
    "description": "Web-Terminal is a terminal server that provides remote CLI via standard web browser and HTTP protocol.",
    "version": "0.5.0",
    "author": "Boyan Rabchev <boyan@rabchev.com>",
    "contributors": [ 
        { "name": "Boyan Rabchev", "email": "boyan@rabchev.com" }
    ],
    "config": {
        "port": "8088",
        "root": "/terminal",
        "requireSSL": "false",
        "key": "",
        "cert": "",
        "pfx": ""
    },
    "repository": "https://github.com/rabchev/web-terminal.git",
    "main": "index",
    "preferGlobal": "false",
    "dependencies": {
        "send": "*",
        "connect": "*",
        "socket.io": "*",
        "commander": "*"
    },
    "devDependencies": {
        "nodeunit": "*"
    },
    "keywords": [
        "command",
        "shell",
        "command-line",
        "text",
        "terminal",
        "browser",
        "web",
        "node"
    ],
    "preferGlobal": "false",
    "bin": {
        "web-terminal": "./bin/run.js"
    },
    "scripts": {
        "start": "node ./bin/run.js"
    },
    "engines": { "node": ">=0.10" }
}